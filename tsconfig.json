{
  "compilerOptions": {
    "target": "ES5",
    "lib": ["DOM","DOM.Iterable","esnext"],
    "esModuleInterop":true,
    "module":"esnext",
    "moduleResolution": "Node",
    "jsx":"react",
    "rootDir": "src",
    "baseUrl": "src",
    "allowJs": true,
    "skipLibCheck": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "isolatedModules":false,
    "noEmit": true
  },
  "include": ["src"],
  "exclude": ["coverage"]
}